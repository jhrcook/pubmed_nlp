doi = {10.1016/j.cell.2012.02.053},
file = {:Users/admin/Documents/Mendeley Desktop/2012 - Duncan et al. - CELL - Dynamic Reprogramming of the Kinome in Response to Targeted MEK Inhibition in Triple-Negative Breast Cance.pdf:pdf},
