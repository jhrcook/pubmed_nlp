pages = {248--9},
pmid = {20354512},
publisher = {Cambridge Univ. Press},
title = {{A method and server for predicting damaging missense mutations.}},
url = {http://cran.r-project.org/web/packages/pcalg/index. http://www.nature.com/articles/nmeth0410-248 http://www.ncbi.nlm.nih.gov/pubmed/20354512 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2855889},
volume = {7},
