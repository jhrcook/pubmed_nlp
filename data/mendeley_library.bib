pages = {262},
pmid = {30001694},
publisher = {BioMed Central},
title = {{Alternative empirical Bayes models for adjusting for batch effects in genomic studies.}},
url = {https://bmcbioinformatics.biomedcentral.com/articles/10.1186/s12859-018-2263-6 http://www.ncbi.nlm.nih.gov/pubmed/30001694 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC6044013},
volume = {19},
