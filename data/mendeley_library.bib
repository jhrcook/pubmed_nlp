file = {:Users/admin/Documents/Mendeley Desktop/2012 - Das, Yu - BMC systems biology - HINT High-quality protein interactomes and their applications in understanding human disease.pdf:pdf},
isbn = {1752-0509 (Electronic)$\backslash$r1752-0509 (Linking)},
issn = {1752-0509},
journal = {BMC systems biology},
keywords = {Disease,Interactomes,Networks,Protein-protein interactions},
month = {jul},
