keywords = {O-miner,data analysis,data integration,multi-omics,sequencing},
month = {aug},
pmid = {28981577},
title = {{'Multi-omic' data analysis using O-miner.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28981577},
year = {2017}
}
