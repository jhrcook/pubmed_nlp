title = {{Compressing Networks with Super Nodes.}},
url = {http://www.nature.com/articles/s41598-018-29174-3 http://www.ncbi.nlm.nih.gov/pubmed/30022035 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC6052167},
