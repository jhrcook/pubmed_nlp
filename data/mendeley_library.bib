issn = {2056-7189},
journal = {NPJ systems biology and applications},
keywords = {Bioenergetics,Cell biology,Computational biology and bioinformatics,Computer modelling,Medical research},
month = {dec},
number = {1},
pages = {29},
pmid = {28983409},
publisher = {Nature Publishing Group},
title = {{Systems biology reveals uncoupling beyond UCP1 in human white fat-derived beige adipocytes.}},
url = {http://www.nature.com/articles/s41540-017-0027-y http://www.ncbi.nlm.nih.gov/pubmed/28983409 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5626775},
volume = {3},
