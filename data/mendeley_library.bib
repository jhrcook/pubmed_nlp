month = {jul},
number = {Suppl 7},
pages = {188},
pmid = {30066650},
publisher = {BioMed Central},
title = {{INBIA: a boosting methodology for proteomic network inference.}},
url = {https://bmcbioinformatics.biomedcentral.com/articles/10.1186/s12859-018-2183-5 http://www.ncbi.nlm.nih.gov/pubmed/30066650 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC6069689},
