title = {{Tissue-specific tumorigenesis: context matters.}},
url = {http://www.nature.com/articles/nrc.2017.5 http://www.ncbi.nlm.nih.gov/pubmed/28256574 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5823237},
volume = {17},
