doi = {10.1093/bioinformatics/bty716},
eprint = {1710.01912},
file = {:Users/admin/Documents/Mendeley Desktop/2019 - Sulaimanov et al. - Bioinformatics (Oxford, England) - Inferring gene expression networks with hubs using a degree weighted Lasso.pdf:pdf},
issn = {1367-4811},
journal = {Bioinformatics (Oxford, England)},
month = {mar},
