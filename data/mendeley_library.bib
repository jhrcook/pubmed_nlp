title = {{MEXCOWalk: Mutual Exclusion and Coverage Based Random Walk to Identify Cancer Modules}},
url = {http://dx.doi.org/10.1101/547653 http://biorxiv.org/cgi/content/short/547653v1},
year = {2015}
}
