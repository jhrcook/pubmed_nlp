pages = {449--56},
pmid = {23689023},
title = {{Stochastic gene expression in mammals: lessons from olfaction.}},
url = {http://dx.doi.org/10.1016/j.tcb.2013.04.005 http://www.ncbi.nlm.nih.gov/pubmed/23689023 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3755038},
volume = {23},
