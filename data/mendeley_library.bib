isbn = {9783939897477},
issn = {18688969},
journal = {FSTTCS 2012 - IARCS Annual Conference on Foundations of Software Technology and Theoretical Computer Science},
keywords = {causality,concurrency,graph rewriting,pathways,rule-based models},
pages = {276--288},
title = {{Graphs, Rewriting and Pathway Reconstruction for Rule-Based Models}},
url = {https://fontana.hms.harvard.edu/sites/fontana.hms.harvard.edu/files/documents/fsttcs12.pdf http://hal.archives-ouvertes.fr/hal-00809065},
volume = {18},
