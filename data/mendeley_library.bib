title = {{FACETS: allele-specific copy number and clonal heterogeneity analysis tool for high-throughput DNA sequencing.}},
url = {https://academic.oup.com/nar/article-abstract/44/16/e131/2460163 http://www.ncbi.nlm.nih.gov/pubmed/27270079 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5027494},
