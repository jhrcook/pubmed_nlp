title = {{Opportunities and challenges for transcriptome-wide association studies.}},
url = {https://doi.org/10.1038/s41588-019-0385-z http://www.nature.com/articles/s41588-019-0385-z http://www.ncbi.nlm.nih.gov/pubmed/30926968},
volume = {51},
