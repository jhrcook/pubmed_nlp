title = {{KRAS G12D Mutation Subtype Is A Prognostic Factor for Advanced Pancreatic Adenocarcinoma.}},
url = {www.nature.com/ctg http://www.nature.com/doifinder/10.1038/ctg.2016.18 http://www.ncbi.nlm.nih.gov/pubmed/27010960 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4822095},
volume = {7},
