title = {{Associating genes and protein complexes with disease via network propagation.}},
url = {http://safrabio.cs.tau.ac.il http://www.ncbi.nlm.nih.gov/pubmed/20090828 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2797085},
volume = {6},
