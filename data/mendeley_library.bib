pmid = {26038887},
title = {{Association Between Specific Mutations in KRAS Codon 12 and Colorectal Liver Metastasis.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26038887 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4696064},
volume = {150},
year = {2015}
}
