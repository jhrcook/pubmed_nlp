file = {:Users/admin/Documents/Mendeley Desktop/2019 - van Mierlo et al. - Cell stem cell - Integrative Proteomic Profiling Reveals PRC2-Dependent Epigenetic Crosstalk Maintains Ground.pdf:pdf},
issn = {1875-9777},
journal = {Cell stem cell},
keywords = {H3K27me3,chromatin profiling,embryonic stem cells,epigenetics,ground-state pluripotency,histone modifications},
month = {jan},
