journal = {Cell death and differentiation},
keywords = {Bax,Bcl-X S,R116C,R120G,aA-crystallin,aB-crystallin},
month = {may},
number = {5},
pages = {512--26},
pmid = {14752512},
title = {{Human alphaA- and alphaB-crystallins bind to Bax and Bcl-X(S) to sequester their translocation during staurosporine-induced apoptosis.}},
