pmid = {28168295},
title = {{Discovery of cancer common and specific driver gene sets.}},
url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC5449640/pdf/gkx089.pdf http://www.ncbi.nlm.nih.gov/pubmed/28168295 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5449640},
volume = {45},
year = {2017}
}
