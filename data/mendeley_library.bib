publisher = {Nature Publishing Group},
title = {{Enhancer histone-QTLs are enriched on autoimmune risk haplotypes and influence gene expression within chromatin networks.}},
url = {http://www.nature.com/articles/s41467-018-05328-9 http://www.ncbi.nlm.nih.gov/pubmed/30046115 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC6060153},
