journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {cancer,cancer evolution,cancer incidence,driver mutations,somatic mutation rate},
month = {jan},
number = {1},
pages = {118--23},
pmid = {25535351},
title = {{Only three driver gene mutations are required for the development of lung and colorectal cancers.}},
url = {https://www.nature.com/articles/onc2014223.pdf http://www.ncbi.nlm.nih.gov/pubmed/25065594 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4836617 http://www.ncbi.nlm.nih.gov/pubmed/25535351 http://www.pubmedcentral.nih.gov/articlerender.fcgi},
volume = {112},
