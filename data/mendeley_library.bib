title = {{Time series modeling of live-cell shape dynamics for image-based phenotypic profiling.}},
url = {http://pubs.rsc.org/en/content/articlepdf/2016/ib/c5ib00283d http://xlink.rsc.org/?DOI=C5IB00283D http://www.ncbi.nlm.nih.gov/pubmed/26658688 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5058786},
