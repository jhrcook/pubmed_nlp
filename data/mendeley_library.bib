doi = {10.1186/s12859-019-2627-6},
file = {:Users/admin/Documents/Mendeley Desktop/2019 - Luttrell et al. - BMC bioinformatics - Predicting protein residue-residue contacts using random forests and deep networks.pdf:pdf},
issn = {1471-2105},
journal = {BMC bioinformatics},
