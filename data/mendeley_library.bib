keywords = {CAV1,CAVIN1,Hippo pathway,TAZ,YAP,caveolae,extra cellular matrix,mechanotransduction,shear stress},
month = {jan},
number = {2},
pages = {242--255.e6},
pmid = {30595521},
publisher = {Elsevier},
title = {{The Hippo Pathway Regulates Caveolae Expression and Mediates Flow Response via Caveolae.}},
