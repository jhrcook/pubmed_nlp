file = {:Users/admin/Documents/Mendeley Desktop/2019 - K{\"{o}}ster, Brown, Liu - Bioinformatics (Oxford, England) - A Bayesian model for single cell transcript expression analysis on MERFI.pdf:pdf},
issn = {1367-4811},
journal = {Bioinformatics (Oxford, England)},
month = {mar},
number = {6},
pages = {995--1001},
pmid = {30875429},
title = {{A Bayesian model for single cell transcript expression analysis on MERFISH data.}},
url = {https://doi.org/10.1016/j.cell.2019.01.031 https://linkinghub.elsevier.com/retrieve/pii/S0092867419300947 http://www.ncbi.nlm.nih.gov/pubmed/30827681 http://www.ncbi.nlm.nih.gov/pubmed/30875429},
volume = {35},
