isbn = {0027-8424},
issn = {1549-5469},
journal = {Genome research},
number = {8},
pages = {1312--1322},
pmid = {28487279},
title = {{Single-cell sequencing deciphers a convergent evolution of copy number alterations from primary to circulating tumor cells.}},
url = {http://www.genome.org/cgi/doi/10.1101/gr.216788.116. http://www.ncbi.nlm.nih.gov/pubmed/28487279 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5538548},
volume = {27},
