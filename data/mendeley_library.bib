pmid = {19535538},
title = {{A statistical framework for protein quantitation in bottom-up MS-based proteomics.}},
url = {http://omics.pnl.gov/software/ http://www.ncbi.nlm.nih.gov/pubmed/19535538 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2723007},
volume = {25},
