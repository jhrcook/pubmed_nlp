pages = {1993--2001},
pmid = {25855118},
title = {{Review, evaluation, and discussion of the challenges of missing value imputation for mass spectrometry-based label-free global proteomics.}},
url = {http://www.ii.uib.no/{~}trondb/imputation/ http://www.ncbi.nlm.nih.gov/pubmed/25855118 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4776766},
