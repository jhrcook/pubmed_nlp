@article{Waters2018,
abstract = {RAS genes (HRAS, KRAS, and NRAS) comprise the most frequently mutated oncogene family in human cancer. With the highest RAS mutation frequencies seen with the top three causes of cancer deaths in the United States (lung, colorectal, and pancreatic cancer), the development of anti-RAS therapies is a major priority for cancer research. Despite more than three decades of intense effort, no effective RAS inhibitors have yet to reach the cancer patient. With bitter lessons learned from past failures and with new ideas and strategies, there is renewed hope that undruggable RAS may finally be conquered. With the KRAS isoform mutated in 84{\%} of all RAS-mutant cancers, we focus on KRAS. With a near 100{\%} KRAS mutation frequency, pancreatic ductal adenocarcinoma (PDAC) is considered the most RAS-addicted of all cancers. We review the role of KRAS as a driver and therapeutic target in PDAC.},
author = {Waters, Andrew M and Der, Channing J},
