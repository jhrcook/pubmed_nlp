pmid = {30898383},
title = {{Governance of automated image analysis and artificial intelligence analytics in healthcare.}},
url = {https://doi.org/10.1016/j.crad.2019.02.005 http://www.ncbi.nlm.nih.gov/pubmed/30898383},
volume = {74},
