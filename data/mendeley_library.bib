publisher = {BioMed Central},
title = {{Similarities and differences between variants called with human reference genome HG19 or HG38.}},
url = {https://bmcbioinformatics.biomedcentral.com/articles/10.1186/s12859-019-2620-0 http://link.springer.com/article/10.1186/s12859-019-2620-0?utm{\_}source=researcher{\_}app{\&}utm{\_}medium=referral{\&}utm{\_}campaign=MKEF{\_}USG{\_}Researcher{\_}inbound http://www.ncbi.nlm.nih.gov/pu},
