issn = {1096-9896},
journal = {The Journal of pathology},
keywords = {BCL2,KRAS,PIK3CD,apoptosis,colorectal cancer,miR-30b,proliferation},
month = {mar},
number = {4},
pages = {415--27},
pmid = {24293274},
title = {{MicroRNA-30b functions as a tumour suppressor in human colorectal cancer by targeting KRAS, PIK3CD and BCL2.}},
