eprint = {1802.01933},
file = {:Users/admin/Documents/Mendeley Desktop/2018 - Guidotti et al. - ACM Comput. Surv - A Survey Of Methods For Explaining Black Box Models.pdf:pdf},
journal = {ACM Comput. Surv},
keywords = {Additional Key Words and Phrases: Open the black b,CCS Concepts: • Information systems → Decision sup,Data analytics,Data mining,explanations,interpretability,transparent models ACM Reference format:},
month = {feb},
number = {93},
title = {{A Survey Of Methods For Explaining Black Box Models}},
url = {https://doi.org/10.1145/3236009 http://arxiv.org/abs/1802.01933},
volume = {51},
year = {2018}
