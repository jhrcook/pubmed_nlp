journal = {Drugs},
month = {apr},
pmid = {30968289},
title = {{Targeting EGFR and RAS/RAF Signaling in the Treatment of Metastatic Colorectal Cancer: From Current Treatment Strategies to Future Perspectives.}},
url = {http://cshperspectives.cshlp.org/ http://www.ncbi.nlm.nih.gov/pubmed/30709881 http://www.ncbi.nlm.nih.gov/pubmed/30968289},
year = {2019}
}
