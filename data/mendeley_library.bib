doi = {10.1126/science.aah5043},
file = {:Users/admin/Documents/Mendeley Desktop/2017 - Geller et al. - Science (New York, N.Y.) - Potential role of intratumor bacteria in mediating tumor resistance to the chemotherap.pdf:pdf},
issn = {1095-9203},
