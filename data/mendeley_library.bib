issn = {1949-2553},
journal = {Oncotarget},
keywords = {3′ UTR,KRAS,miR-185,microRNAs (miRNAs),post-transcriptional regulation},
month = {mar},
number = {11},
pages = {11770--84},
pmid = {26930719},
title = {{Cis-acting elements in its 3' UTR mediate post-transcriptional regulation of KRAS.}},
url = {www.impactjournals.com/oncotarget{\%}0Awww.impactjournals.com/oncotarget/ http://www.ncbi.nlm.nih.gov/pubmed/26930719 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4914247},
