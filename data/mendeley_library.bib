title = {{Edge-based scoring and searching method for identifying condition-responsive protein-protein interaction sub-network.}},
url = {https://academic.oup.com/bioinformatics/article-abstract/23/16/2121/198073 http://www.ncbi.nlm.nih.gov/pubmed/17545181},
