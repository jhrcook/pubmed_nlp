journal = {Journal of gastrointestinal surgery : official journal of the Society for Surgery of the Alimentary Tract},
keywords = {Intra-pancreatic distal bile duct carcinoma,KRAS,Pancreatic ductal adenocarcinoma},
month = {may},
number = {5},
pages = {953--9},
pmid = {26956004},
title = {{Intra-pancreatic Distal Bile Duct Carcinoma is Morphologically, Genetically, and Clinically Distinct from Pancreatic Ductal Adenocarcinoma.}},
