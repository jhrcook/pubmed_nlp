title = {{Predicting ligand-dependent tumors from multi-dimensional signaling features.}},
url = {http://www.nature.com/articles/s41540-017-0030-3 http://www.ncbi.nlm.nih.gov/pubmed/28944080 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5607260},
volume = {3},
