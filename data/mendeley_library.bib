issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {KRAS,MAPK,RAF,autophagy,siRNA},
month = {feb},
pages = {201817494},
pmid = {30709910},
title = {{MAP kinase and autophagy pathways cooperate to maintain RAS mutant cancer cell survival.}},
url = {www.pnas.org/cgi/doi/10.1073/pnas.1817494116 http://www.ncbi.nlm.nih.gov/pubmed/30709910},
year = {2019}
}
