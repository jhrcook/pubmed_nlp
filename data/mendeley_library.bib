publisher = {Papers in Press},
title = {{Codon usage regulates human KRAS expression at both transcriptional and translational levels.}},
url = {http://www.jbc.org/ http://www.ncbi.nlm.nih.gov/pubmed/30275015 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC6240855},
volume = {293},
