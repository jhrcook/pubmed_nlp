journal = {The Journal of biological chemistry},
keywords = {Ras protein,allosteric regulation,conformational change,enzyme catalysis,enzyme structure,oncogene},
number = {31},
pages = {12981--12993},
pmid = {28630043},
title = {{The small GTPases K-Ras, N-Ras, and H-Ras have distinct biochemical properties determined by allosteric effects.}},
url = {http://www.jbc.org/ http://www.ncbi.nlm.nih.gov/pubmed/28630043 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5546037},
volume = {292},
