pmid = {21746896},
title = {{Systematic investigation of genetic vulnerabilities across cancer cell lines reveals lineage-specific dependencies in ovarian cancer.}},
url = {http://www.pnas.org/content/pnas/108/30/12372.full.pdf http://www.ncbi.nlm.nih.gov/pubmed/21746896 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3145679},
