issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {chemotaxis,excitability,phosphoinositides,positive feedback loop,signal transduction},
number = {39},
pages = {E9125--E9134},
pmid = {30194235},
title = {{Mutually inhibitory Ras-PI(3,4)P2 feedback loops mediate cell migration.}},
url = {www.pnas.org/cgi/doi/10.1073/pnas.1809039115 http://feedproxy.google.com/{~}r/Pnas-RssFeedOfEarlyEditionArticles/{~}3/lgCtOWcE5Oc/1809039115.short http://www.ncbi.nlm.nih.gov/pubmed/30194235},
