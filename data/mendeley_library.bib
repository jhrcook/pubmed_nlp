eprint = {044925},
file = {:Users/admin/Documents/Mendeley Desktop/2017 - Hernandez-Armenta et al. - Bioinformatics (Oxford, England) - Benchmarking substrate-based kinase activity inference using phosph.pdf:pdf},
isbn = {8446004402},
issn = {1367-4811},
journal = {Bioinformatics (Oxford, England)},
month = {jun},
number = {12},
pages = {1845--1851},
