pmid = {28125762},
title = {{Association of Dietary Patterns With Risk of Colorectal Cancer Subtypes Classified by Fusobacterium nucleatum in Tumor Tissue.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28125762 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5502000},
volume = {3},
year = {2017}
}
@article{Zwang2017,
abstract = {Activating mutations involving the PI3K pathway occur frequently in human cancers. However, PI3K inhibitors primarily induce cell cycle arrest, leaving a significant reservoir of tumor cells that may acquire or exhibit resistance. We searched for genes that are required for the survival of PI3K mutant cancer cells in the presence of PI3K inhibition by conducting a genome scale shRNA-based apoptosis screen in a PIK3CA mutant human breast cancer cell. We identified 5 genes (PIM2, ZAK, TACC1, ZFR, ZNF565) whose suppression induced cell death upon PI3K inhibition. We showed that small molecule inhibitors of the PIM2 and ZAK kinases synergize with PI3K inhibition. In addition, using a microscale implementable device to deliver either siRNAs or small molecule inhibitors in vivo, we showed that suppressing these 5 genes with PI3K inhibition induced tumor regression. These observations identify targets whose inhibition synergizes with PI3K inhibitors and nominate potential combination therapies involving PI3K inhibition.},
author = {Zwang, Yaara and Jonas, Oliver and Chen, Casandra and Rinne, Mikael L. and Doench, John G. and Piccioni, Federica and Tan, Li and Huang, Hai-Tsang and Wang, Jinhua and Ham, Young Jin and O'Connell, Joyce and Bhola, Patrick and Doshi, Mihir and Whitman, Matthew and Cima, Michael and Letai, Anthony and Root, David E. and Langer, Robert S. and Gray, Nathanael and Hahn, William C.},
