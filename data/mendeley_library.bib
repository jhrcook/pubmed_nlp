pmid = {24440911},
title = {{Multiple receptor tyrosine kinases converge on microRNA-134 to control KRAS, STAT5B, and glioblastoma.}},
url = {www.nature.com/cdd http://www.ncbi.nlm.nih.gov/pubmed/24440911 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3978301},
volume = {21},
