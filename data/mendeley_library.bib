journal = {Nucleic acids research},
number = {17},
pages = {4925--36},
pmid = {16982645},
title = {{Computational analysis of tissue-specific combinatorial gene regulation: predicting interaction between transcription factors in human tissues.}},
url = {https://academic.oup.com/nar/article-abstract/34/17/4925/3111971 http://www.ncbi.nlm.nih.gov/pubmed/16982645 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC1635265},
volume = {34},
year = {2006}
}
@article{Dezso2008,
abstract = {BACKGROUND In recent years, the maturation of microarray technology has allowed the genome-wide analysis of gene expression patterns to identify tissue-specific and ubiquitously expressed ('housekeeping') genes. We have performed a functional and topological analysis of housekeeping and tissue-specific networks to identify universally necessary biological processes, and those unique to or characteristic of particular tissues. RESULTS We measured whole genome expression in 31 human tissues, identifying 2374 housekeeping genes expressed in all tissues, and genes uniquely expressed in each tissue. Comprehensive functional analysis showed that the housekeeping set is substantially larger than previously thought, and is enriched with vital processes such as oxidative phosphorylation, ubiquitin-dependent proteolysis, translation and energy metabolism. Network topology of the housekeeping network was characterized by higher connectivity and shorter paths between the proteins than the global network. Ontology enrichment scoring and network topology of tissue-specific genes were consistent with each tissue's function and expression patterns clustered together in accordance with tissue origin. Tissue-specific genes were twice as likely as housekeeping genes to be drug targets, allowing the identification of tissue 'signature networks' that will facilitate the discovery of new therapeutic targets and biomarkers of tissue-targeted diseases. CONCLUSION A comprehensive functional analysis of housekeeping and tissue-specific genes showed that the biological function of housekeeping and tissue-specific genes was consistent with tissue origin. Network analysis revealed that tissue-specific networks have distinct network properties related to each tissue's function. Tissue 'signature networks' promise to be a rich source of targets and biomarkers for disease treatment and diagnosis.},
author = {Dezso, Zolt{\'{a}}n and Nikolsky, Yuri and Sviridov, Evgeny and Shi, Weiwei and Serebriyskaya, Tatiana and Dosymbekov, Damir and Bugrim, Andrej and Rakhmatulin, Eugene and Brennan, Richard J and Guryanov, Alexey and Li, Kelly and Blake, Julie and Samaha, Raymond R and Nikolskaya, Tatiana},
doi = {10.1186/1741-7007-6-49},
file = {:Users/admin/Documents/Mendeley Desktop/2008 - Dezso et al. - BMC biology - A comprehensive functional analysis of tissue specificity of human gene expression.pdf:pdf},
issn = {1741-7007},
journal = {BMC biology},
month = {nov},
pages = {49},
pmid = {19014478},
title = {{A comprehensive functional analysis of tissue specificity of human gene expression.}},
url = {http://www.biomedcentral.com/1741-7007/6/49 http://www.ncbi.nlm.nih.gov/pubmed/19014478 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2645369},
volume = {6},
