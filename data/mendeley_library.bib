publisher = {Nature Publishing Group},
title = {{Dynamic interplay between enhancer-promoter topology and gene activity.}},
url = {http://www.nature.com/articles/s41588-018-0175-z http://www.ncbi.nlm.nih.gov/pubmed/30038397 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC6119122},
volume = {50},
