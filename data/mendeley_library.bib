-- maybe can adapt to align co-mutation with PPI?},
author = {Vanunu, Oron and Magger, Oded and Ruppin, Eytan and Shlomi, Tomer and Sharan, Roded},
doi = {10.1371/journal.pcbi.1000641},
file = {:Users/admin/Documents/Mendeley Desktop/2010 - Vanunu et al. - PLoS computational biology - Associating genes and protein complexes with disease via network propagation.pdf:pdf},
isbn = {1553-7358 (Electronic)$\backslash$r1553-734X (Linking)},
issn = {1553-7358},
journal = {PLoS computational biology},
month = {jan},
number = {1},
pages = {e1000641},
