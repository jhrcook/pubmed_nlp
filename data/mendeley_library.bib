title = {{Multiplexed droplet single-cell RNA-sequencing using natural genetic variation.}},
url = {http://www.nature.com/doifinder/10.1038/nbt.4042 http://www.ncbi.nlm.nih.gov/pubmed/29227470 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5784859},
