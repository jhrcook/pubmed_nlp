issn = {1687-4145},
journal = {EURASIP journal on bioinformatics {\&} systems biology},
keywords = {-Omics data,Co-expression network,PPI network,Pearson's correlation,Systems biology,Topological analysis,WGCNA},
month = {dec},
number = {1},
pages = {6},
pmid = {28477207},
title = {{From protein-protein interactions to protein co-expression networks: a new perspective to evaluate large-scale proteomic data.}},
url = {https://bsb-eurasipjournals.springeropen.com/track/pdf/10.1186/s13637-017-0059-z http://www.ncbi.nlm.nih.gov/pubmed/28477207 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5359264},
volume = {2017},
